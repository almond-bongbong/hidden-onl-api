{
  "name": "hidden-onl-api",
  "version": "1.0.0",
  "description": "graphql API server for hidden onl",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec ts-node src/index.ts -e ts,graphql",
    "premigrate": "npx prisma migrate save --name update --experimental",
    "migrate": "npx prisma migrate up --experimental",
    "generate:prisma": "npx prisma generate",
    "generate:type": "graphql-codegen --config codegen.yml",
    "compile": "tsc"
  },
  "author": "max",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^2.3.0",
    "dotenv": "^8.2.0",
    "graphql-tools": "^6.0.15",
    "graphql-yoga": "^1.18.3",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.6",
    "@graphql-codegen/introspection": "1.17.6",
    "@graphql-codegen/typescript": "1.17.6",
    "@graphql-codegen/typescript-resolvers": "1.17.6",
    "@prisma/cli": "^2.3.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^13.11.1",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gts": "^2.0.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  }
}
