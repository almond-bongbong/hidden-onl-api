    "graphql-yoga": "^3.6.0",
    "jsonwebtoken": "^9.0.0",
    "merge-graphql-schemas": "^1.7.8",
    "moment": "^2.29.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "1.17.6",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-resolvers": "1.20.0",
    "@prisma/cli": "^2.4.1",
