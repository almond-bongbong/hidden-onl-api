{
  "name": "hidden-onl-api",
  "version": "1.0.0",
  "description": "graphql API server for hidden onlã…£",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register --files src/index.ts -e ts,graphql",
    "premigrate": "npx prisma migrate save --name update --experimental",
    "schema": "npm run migrate && npm run generate:prisma && npm run generate:type",
    "migrate": "npx prisma migrate up --experimental",
    "generate:prisma": "npx prisma generate",
    "generate:type": "graphql-codegen --config codegen.yml",
    "compile": "tsc"
  },
